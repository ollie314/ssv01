<%= simple_form_for(@language) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :iso2 %>
    <%= f.input :iso3 %>
    <%= f.input :ietf %>
    <%= f.input :name %>
    <%= f.input :flag %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
